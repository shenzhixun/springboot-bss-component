<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ejet.bi.dynamicservice.mapper.BiResourceCategoryDao">

	<sql id="SqlWhere">
		<where>
			1=1
			<if test="id!=null ">and id=#{id} </if>
			<if test="uuid!=null and uuid!='' ">and uuid=#{uuid} </if>
			<if test="name!=null and name!='' ">and name=#{name} </if>
			<if test="status!=null and status!='' ">and status=#{status} </if>
			<if test="remark!=null and remark!='' ">and remark=#{remark} </if>
			<if test="modifyTime!=null and modifyTime!='' ">and modify_time=#{modifyTime} </if>
			<if test="modifyUser!=null and modifyUser!='' ">and modify_user=#{modifyUser} </if>
			<if test="ext!=null and ext!='' ">and ext=#{ext} </if>
		</where>
	</sql>
	<sql id="SqlWherePK">
		<where>
			1=1
			<if test="id!=null ">and id=#{id} </if>
		</where>
	</sql>
	<sql id="SqlSet">
		<set>
			<if test="id!=null "> id=#{id}, </if>
			<if test="uuid!=null and uuid!='' "> uuid=#{uuid}, </if>
			<if test="name!=null and name!='' "> name=#{name}, </if>
			<if test="status!=null and status!='' "> status=#{status}, </if>
			<if test="remark!=null and remark!='' "> remark=#{remark}, </if>
			<if test="modifyTime!=null and modifyTime!='' "> modify_time=#{modifyTime}, </if>
			<if test="modifyUser!=null and modifyUser!='' "> modify_user=#{modifyUser}, </if>
			<if test="ext!=null and ext!='' "> ext=#{ext}, </if>
		</set>
	</sql>


	<insert  id="insertAutoKey" parameterType="com.ejet.bi.dynamicservice.model.BiResourceCategoryModel" useGeneratedKeys="true" keyProperty="id" >
		 INSERT INTO bi_resource_category (
 			uuid, name, status, remark, 
 			modify_time, modify_user, ext ) 	 VALUES ( 
 			#{uuid}, #{name}, #{status}, #{remark}, 
 			#{modifyTime}, #{modifyUser}, #{ext} ) 
	</insert>

	<insert  id="insertSingle" parameterType="com.ejet.bi.dynamicservice.model.BiResourceCategoryModel" useGeneratedKeys="true" keyProperty="id" >
		 INSERT INTO bi_resource_category (
 			uuid, name, status, remark, 
 			modify_time, modify_user, ext,  id ) 
		 VALUES( 
 			#{uuid}, #{name}, #{status}, #{remark}, 
 			#{modifyTime}, #{modifyUser}, #{ext},  #{id} ) 
	</insert>

	<delete  id="delete" parameterType="com.ejet.bi.dynamicservice.model.BiResourceCategoryModel" >
		 DELETE FROM bi_resource_category
		 <include refid="SqlWhere"></include> 
	</delete>

	<update  id="update" parameterType="com.ejet.bi.dynamicservice.model.BiResourceCategoryModel" >
		 UPDATE bi_resource_category
		 <include refid="SqlSet"></include> 
		 <include refid="SqlWherePK"></include> 
	</update>

	<select  id="findByPK" parameterType="com.ejet.bi.dynamicservice.model.BiResourceCategoryModel" resultType="com.ejet.bi.dynamicservice.model.BiResourceCategoryModel" >
		 SELECT * FROM bi_resource_category
		 <include refid="SqlWhere"></include> 
		 <if test=" orderCond !=null ">  order by ${orderCond} </if> 
	</select>

	<select  id="queryByCond" parameterType="com.ejet.bi.dynamicservice.model.BiResourceCategoryModel" resultType="com.ejet.bi.dynamicservice.model.BiResourceCategoryModel" >
		 SELECT * FROM bi_resource_category
		 <include refid="SqlWhere"></include> 
		 <if test=" orderCond !=null ">  order by ${orderCond} </if> 
	</select>

	<select  id="queryTree" parameterType="com.ejet.bi.dynamicservice.model.BiResourceCategoryModel" resultType="com.ejet.bi.dynamicservice.model.BiResourceCategoryModel" >
		 SELECT * FROM bi_resource_category
		 <include refid="SqlWhere"></include> 
		 <if test=" extSQL !=null "> ${extSQL} </if> 
		 <if test=" orderCond !=null ">  order by ${orderCond} </if> 
	</select>

	<select  id="queryByPage" parameterType="com.ejet.bi.dynamicservice.model.BiResourceCategoryModel" resultType="com.ejet.bi.dynamicservice.model.BiResourceCategoryModel" >
		 SELECT * FROM bi_resource_category
		 <include refid="SqlWhere"></include> 
		 <if test="extSQL!=null "> ${extSQL} </if> 
	</select>

	<select  id="findMaxId" parameterType="com.ejet.bi.dynamicservice.model.BiResourceCategoryModel" resultType="int" >
		 SELECT max(id) FROM bi_resource_category
		 <include refid="SqlWhere"></include> 
	</select>

</mapper>