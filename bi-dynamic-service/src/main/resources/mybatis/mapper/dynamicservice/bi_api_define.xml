	<insert  id="insertAutoKey" parameterType="com.ejet.bi.dynamicservice.model.BiApiDefineModel" useGeneratedKeys="true" keyProperty="id" >
		 INSERT INTO bi_api_define (
 			uuid, api_category_uuid, name, path, 
 			url, prefix, resource_uuid, status, remark, 
 			modify_time, modify_user, ext, ext1, ext2 ) 
