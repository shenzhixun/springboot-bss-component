    <select  id="queryApi" parameterType="com.ejet.bi.dynamicservice.vo.BiApiVO" resultType="com.ejet.bi.dynamicservice.vo.BiApiVO" >
        SELECT C.uuid as api_category_uuid, C.`name` as api_category_name, C.url_prefix,
        API.uuid, API.`name`, API.path, API.url, API.prefix, API.resource_uuid
        FROM bi_api_category C, bi_api_define API
        WHERE C.uuid=API.api_category_uuid
        <if test="id!=null ">and API.id=#{id} </if>
        <if test="uuid!=null and uuid!='' ">and API.uuid=#{uuid} </if>
        <if test="apiCategoryUuid!=null and apiCategoryUuid!='' ">and API.api_category_uuid=#{apiCategoryUuid} </if>
